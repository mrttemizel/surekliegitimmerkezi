<definitions
        name="Universite"
        targetNamespace="http://standart.turkiye.gov.tr/universite/v3"
        xmlns="http://schemas.xmlsoap.org/wsdl/"
        xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:tns="http://standart.turkiye.gov.tr/universite/v3"
        xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
        xmlns:xmime="http://www.w3.org/2005/05/xmlmime" >
    <types>

        <xsd:schema targetNamespace="http://standart.turkiye.gov.tr/universite/v3" xmlns:tns="http://standart.turkiye.gov.tr/universite/v3">

            <xsd:complexType name="sorguParametreType">
                <xsd:sequence>
                    <xsd:element name="kurumKodu" type="xsd:string" />
                    <xsd:element name="kullaniciAdi" type="xsd:string"/>
                    <xsd:element name="sifre" type="xsd:string"/>
                    <xsd:element name="ipAdresi" type="xsd:string"/>

                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="kisiselSorgulamaGType">
                <xsd:complexContent>
                    <xsd:extension base="tns:sorguParametreType">
                        <xsd:sequence>
                            <xsd:element name="tcKimlikNo" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="islemSonucType">
                <xsd:sequence>
                    <xsd:element name="kurumKodu"  type="xsd:string"/>
                    <xsd:element name="sonucKodu" nillable="false" type="xsd:string"/>
                    <xsd:element name="sonucAciklamasi" nillable="false" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="detailType">
                <xsd:sequence>
                    <xsd:element name="group" type="xsd:string"/>
                    <xsd:element name="key" type="xsd:string"/>
                    <xsd:element name="value" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="arrayOfDetailType">
                <xsd:sequence>
                    <xsd:element name="detay" type="tns:detailType" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="ayarOkuGType">
                <xsd:complexContent>
                    <xsd:extension base="tns:sorguParametreType">
                        <xsd:sequence>
                            <xsd:element name="ayar" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="ayarOkuCType">
                <xsd:complexContent>
                    <xsd:extension base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="deger" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="ayarOkuListeCType">
                <xsd:complexContent>
                    <xsd:extension base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="detayListesi" type="tns:arrayOfDetailType"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="dilSorgula">
                <xsd:sequence>
                    <xsd:element name="dilKey" type="xsd:string"/>
                    <xsd:element name="dilValue" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="arrayOfDilType">
                <xsd:sequence>
                    <xsd:element name="dil" type="tns:dilSorgula" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="dilSorgulaGType">
                <xsd:complexContent>
                    <xsd:extension base="tns:sorguParametreType">
                        <xsd:sequence>
                            <xsd:element name="belgeTur" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="dilSorgulaCType">
                <xsd:complexContent>
                    <xsd:extension base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="dilListesi" type="tns:arrayOfDilType"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="ogrencilikBilgisiSorgulamaGType">
                <xsd:complexContent>
                    <xsd:extension base="tns:kisiselSorgulamaGType">
                        <xsd:sequence>
                            <xsd:element name="hizmetTuru" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="ogrencilikBilgisiType">
                <xsd:sequence>
                    <xsd:element name="ogrenciNo" type="xsd:string"/>
                    <xsd:element name="program" type="xsd:string"/>
                    <xsd:element name="programKodu" type="xsd:string"/>
                    <xsd:element name="kayitTarihi" type="xsd:date"/>
                    <xsd:element name="ogrencilikDurumu" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="arrayOfOgrencilikBilgisiType">
                <xsd:sequence>
                    <xsd:element name="ogrencilikBilgisi" maxOccurs="unbounded" minOccurs="0" type="tns:ogrencilikBilgisiType"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="ogrenciBilgisiListesiSorgulamaCType">
                <xsd:complexContent>
                    <xsd:extension base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="ogrenciBilgisiListesi" type="tns:arrayOfOgrencilikBilgisiType"/>
                            <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="universiteBelgeOlusturGType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:kisiselSorgulamaGType">
                        <xsd:sequence>
                            <xsd:element name="ogrenciNo" type="xsd:string"/>
                            <xsd:element name="programKodu" type="xsd:string"/>
                            <xsd:element name="belgeTur" type="xsd:string"/>
                            <xsd:element name="donem" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="true"/>
                            <xsd:element name="dilKey" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="kayitliBelgeSorgulaGType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:kisiselSorgulamaGType">
                        <xsd:sequence>
                            <xsd:element name="belgeTur" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="universiteBelgeOlusturCType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="belge" type="xsd:base64Binary"/>
                            <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>


            <xsd:complexType name="kayitliBelgeType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:ogrencilikBilgisiType">
                        <xsd:sequence>
                            <xsd:element name="barkodNo" type="xsd:string"/>
                            <xsd:element name="olusturulmaTarihi" type="xsd:dateTime"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="arrayOfKayitliBelge">
                <xsd:sequence>
                    <xsd:element name="kayitliBelge" type="tns:kayitliBelgeType" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="KayitliBelgeSorgulaCType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="kayitliBelgeListesi" type="tns:arrayOfKayitliBelge"/>
                            <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="barkodluBelgeDogrulaGType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:sorguParametreType">
                        <xsd:sequence>
                            <xsd:element name="tcKimlikNo" type="xsd:string"/>
                            <xsd:element name="barkodNo" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="barkodluBelgeDogrulaCType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="belge" type="xsd:base64Binary"/>
                            <xsd:element name="tcKimlikNo" type="xsd:string"/>
                            <xsd:element name="ad" type="xsd:string"/>
                            <xsd:element name="soyad" type="xsd:string"/>
                            <xsd:element name="belgeOlusturulmaTarihi" type="xsd:dateTime"/>
                            <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="akademikTakvimCType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="genelAciklama" type="xsd:string"/>
                            <xsd:element name="belge" type="xsd:base64Binary"/>
                            <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="sinavSonucuGType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:kisiselSorgulamaGType">
                        <xsd:sequence>
                            <xsd:element name="ogrenciNo" type="xsd:string"/>
                            <xsd:element name="programKodu" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>


            <xsd:complexType name="sinavSonucuCType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="donemListesi" type="tns:arrayOfDonemListesi"/>
                            <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="donemType">
                <xsd:sequence>
                    <xsd:element name="donemAdi" type="xsd:string"/>
                    <xsd:element name="ogretimYili" type="xsd:string"/>
                    <xsd:element name="donemNotOrtalamasi" type="xsd:string"/>
                    <xsd:element name="genelNotOrtalamasi" type="xsd:string"/>
                    <xsd:element name="alinanDersListesi" type="tns:arrayOfAlinanDersListesi"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="arrayOfDonemListesi">
                <xsd:sequence>
                    <xsd:element name="donemBilgisi" type="tns:donemType" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>


            <xsd:complexType name="alinanDersType">
                <xsd:sequence>
                    <xsd:element name="dersKodu" type="xsd:string"/>
                    <xsd:element name="dersAdi" type="xsd:string"/>
                    <xsd:element name="krediAkts" type="xsd:string"/>
                    <xsd:element name="basariNotu" type="xsd:string"/>
                    <xsd:element name="akademikPersonel" type="xsd:string"/>
                    <xsd:element name="harfNotu" type="xsd:string"/>
                    <xsd:element name="dersSinavSonucListesi" type="tns:arrayOfDersSinavSonucListesi"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="arrayOfAlinanDersListesi">
                <xsd:sequence>
                    <xsd:element name="alinanDers" type="tns:alinanDersType" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="dersSinavSonucType">
                <xsd:sequence>
                    <xsd:element name="sinavAdi" type="xsd:string"/>
                    <xsd:element name="sinavSonucu" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="arrayOfDersSinavSonucListesi">
                <xsd:sequence>
                    <xsd:element name="dersSinavSonucu" type="tns:dersSinavSonucType" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="donemNotOrtalamasiGType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:kisiselSorgulamaGType">
                        <xsd:sequence>
                            <xsd:element name="ogrenciNo" type="xsd:string"/>
                            <xsd:element name="programKodu" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>


            <xsd:complexType name="donemNotOrtalamasiCType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="donemNotListesi" type="tns:arrayOfDonemNotListesi"/>
                            <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="donemNotType">
                <xsd:sequence>
                    <xsd:element name="donemTarihi" type="xsd:string"/>
                    <xsd:element name="donemAdi" type="xsd:string"/>
                    <xsd:element name="donemNotOrtalamasi" type="xsd:string"/>
                    <xsd:element name="genelNotOrtalamasi" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="arrayOfDonemNotListesi">
                <xsd:sequence>
                    <xsd:element name="donemNotBilgisi" type="tns:donemNotType" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="danismanBilgisiSorgulamaGType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:kisiselSorgulamaGType">
                        <xsd:sequence>
                            <xsd:element name="ogrenciNo" type="xsd:string"/>
                            <xsd:element name="programKodu" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="danismanBilgisiSorgulamaCType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="danismanAdi" type="xsd:string"/>
                            <xsd:element name="danismanSoyadi" type="xsd:string"/>
                            <xsd:element name="danismanOfisi" type="xsd:string"/>
                            <xsd:element name="danismanTelefon" type="xsd:string"/>
                            <xsd:element name="danismanEposta" type="xsd:string"/>
                            <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="etkinlikBilgisiType">
                <xsd:sequence>
                    <xsd:element name="etkinlikBaslangicTarihi" type="xsd:date"/>
                    <xsd:element name="etkinlikBitisTarihi" type="xsd:date"/>
                    <xsd:element name="etkinlikTuru" type="xsd:string"/>
                    <xsd:element name="etkinlikDuzenleyen" type="xsd:string"/>
                    <xsd:element name="etkinlikAdi" type="xsd:string"/>
                    <xsd:element name="etkinlikAdres" type="xsd:string"/>
                    <xsd:element name="etkinlikAciklama" type="xsd:string"/>
                    <xsd:element name="etkinlikBaslangicSaati" type="xsd:string"/>
                    <xsd:element name="etkinlikBitisSaati" type="xsd:string"/>
                    <xsd:element name="etkinlikTahminiSure" type="xsd:string"/>
                    <xsd:element name="etkinlikMekan" type="xsd:string"/>
                    <xsd:element name="etkinlikKatilimSarti" type="xsd:string"/>
                    <xsd:element name="etkinlikURL" type="xsd:string"/>
                    <xsd:element name="etkinlikKoordinat" nillable="true">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="enlem" type="xsd:string"/>
                                <xsd:element name="boylam" type="xsd:string"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="arrayOfEtkinlikBilgisi">
                <xsd:sequence>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" name="etkinlikBilgisi" type="tns:etkinlikBilgisiType"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="etkinlikBilgisiSorgulaCType">
                <xsd:complexContent>
                    <xsd:extension base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="sorgulamaZamanAralikBilgisi" type="xsd:string"/>
                            <xsd:element name="etkinlikListesi" type="tns:arrayOfEtkinlikBilgisi"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="devamTakipDersListesiType">
                <xsd:sequence>
                    <xsd:element name="donemTarihi" type="xsd:string"/>
                    <xsd:element name="donemAdi" type="xsd:string"/>
                    <xsd:element name="dersAdi" type="xsd:string"/>
                    <xsd:element name="program" type="xsd:string"/>
                    <xsd:element name="devamTakipListesi" type="tns:arrayOfDevamTakipListesi"/>
                    <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="devamTakipType">
                <xsd:sequence>
                    <xsd:element name="tarih" type="xsd:date"/>
                    <xsd:element name="aciklama" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="arrayOfDevamTakipListesi">
                <xsd:sequence>
                    <xsd:element name="devamTakip" type="tns:devamTakipType" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="arrayOfdevamTakipDersBilgisi">
                <xsd:sequence>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" name="devamTakipDers" type="tns:devamTakipDersListesiType"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="devamTakipBilgisiSorgulamaGType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:kisiselSorgulamaGType">
                        <xsd:sequence>
                            <xsd:element name="ogrenciNo" type="xsd:string"/>
                            <xsd:element name="programKodu" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="devamTakipBilgisiSorgulamaCType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="devamTakipDersListesi" type="tns:arrayOfdevamTakipDersBilgisi"/>
                            <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="sertifikaDetayType">
                <xsd:sequence>
                    <xsd:element name="baslik" type="xsd:string"/>
                    <xsd:element name="aciklama" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="arrayOfSertifikaDetayListesi">
                <xsd:sequence>
                    <xsd:element name="sertifikaDetay" type="tns:sertifikaDetayType" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>


            <xsd:complexType name="sertifikaType">
                <xsd:sequence>
                    <xsd:element name="sertifikaID" type="xsd:string"/>
                    <xsd:element name="sertifikaNumarasi" type="xsd:string"/>
                    <xsd:element name="tur" type="xsd:string"/>
                    <xsd:element name="sertifikaAdi" type="xsd:string"/>
                    <xsd:element name="alindigiYer" type="xsd:string"/>
                    <xsd:element name="sertifikaGecerlilikTarih" type="xsd:string"/>
                    <xsd:element name="sertifikaDetayListesi" type="tns:arrayOfSertifikaDetayListesi"/>
                    <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="diplomaType">
                <xsd:sequence>
                    <xsd:element name="fakulteAdi" type="xsd:string"/>
                    <xsd:element name="bolumAdi" type="xsd:string"/>
                    <xsd:element name="mezuniyetYili" type="xsd:string"/>
                    <xsd:element name="eDiplomaVarMi" type="xsd:boolean"/>
                    <xsd:element name="aciklama" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="arrayOfSertifikaListesi">
                <xsd:sequence>
                    <xsd:element name="sertifika" type="tns:sertifikaType" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="arrayOfDiplomaListesi">
                <xsd:sequence>
                    <xsd:element name="diploma" type="tns:diplomaType" maxOccurs="unbounded" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="sertifikaSorgulaCType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="sertifikaListesi" type="tns:arrayOfSertifikaListesi"/>
                            <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="diplomaSorgulaGType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:kisiselSorgulamaGType">
                        <xsd:sequence>
                            <xsd:element name="eDiplomaMi" type="xsd:boolean"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="diplomaSorgulaCType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="diplomaListesi" type="tns:arrayOfDiplomaListesi"/>
                            <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="sertifikaBelgeIndirGType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:kisiselSorgulamaGType">
                        <xsd:sequence>
                            <xsd:element name="sertifikaID" type="xsd:string"/>
                            <xsd:element name="dilKey" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>

            <xsd:complexType name="sertifikaBelgeIndirCType">
                <xsd:complexContent>
                    <xsd:extension  base="tns:islemSonucType">
                        <xsd:sequence>
                            <xsd:element name="belge" type="xsd:base64Binary"/>
                            <xsd:element minOccurs="0" name="detayListesi" nillable="true" type="tns:arrayOfDetailType"/>
                        </xsd:sequence>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>


            <xsd:complexType name="donemSorgulaType">
